<section>
      <div class="container-flex mt-5 pt-2 content">
        <div class="row pl-3 ">
            <div class="col-lg-12 col-md-6 col-xs-4 bgColor">
                <table class="table">
  <thead>
    <tr>
      <th scope="col">Student Name</th>
      <th scope="col">Event Name</th>
      <th scope="col">Mail Id</th>
      <th scope="col">Status</th>
    </tr>
  </thead>
 
  <tbody>
      {{#each details}}
    <tr>
     <td>{{this.studName}}</td>
      <td>{{this.eventName}}</td>
      <td>{{this.studEmail}}</td>
      <td>{{this.status}}</td>
    </tr>
    {{/each}}
    </tbody>
    </table>
    </div>
    </div>
    </div>
    </section>