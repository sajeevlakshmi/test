<section>
 <div class="row ">
            <div class="col-md-2" style="width: 200px;"></div>
            <div class=" col-md-10 " style="padding-left: 117px;">
        <h2 class="text-center">PROFILE</h2>


        {{#each details}}
        <form action="/teacher/teacher_Profile" method="post" enctype="multipart/form-data">

          <div id="right-side">
            <label for=""></label>
            <img src="/profile_image/{{this._id}}.jpg" id="profilepic" >

          </div>
          <div class="form-group">
            <label for="name">Name</label>
            <input type="text" class="form-control" name="name" value="{{this.name}}" readonly>


            <label for="">Job</label>
            <input type="text" class="form-control" name="job" value="{{this.job}}" readonly>


            <label for="">Class</label>
            <input type="text" class="form-control" name="class" value="{{this.class}}" readonly>

            <label for="">Address</label>
            <input type="text" class="form-control" name="address" value="{{this.address}}" readonly>

            <label for="">Mobile</label>
            <input type="text" class="form-control" name="mobile" value="{{this.mobile}}" readonly <label
              for="">Email</label>
            <input type="email" class="form-control" name="email" value="{{this.email}}" readonly>

            <button type="submit" class="btn btn-success mt-4" >Edit Profile</button>
          </div>
        </form>
        {{/each}}

      </div>
    </div>

</section>